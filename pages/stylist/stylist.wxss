.container {
    font-family: 'Microsoft yahei',sans-serif;
}

.box-gb {
    width: 100%;
    font-size: 0;
}

.box-gb image {
    width: 100%;
    height: 100%;
}

.box-stylist .box-ul {
    margin-top: 30rpx;
}

.box-stylist .box-ul .box-li {
    position: relative;
    height: 130rpx;
    padding: 30rpx 100rpx 30rpx 190rpx;
    box-shadow: 0 0 6px 1px #eee;
    margin-bottom: 30rpx;
}

.box-stylist .box-ul .box-li .box-img {
    position: absolute;
    top: 20rpx;
    left: 20rpx;
    width: 150rpx;
    height: 150rpx;
}

.box-stylist .box-ul .box-li .box-img image {
    width: 100%;
    height: 100%;
}

.box-stylist .box-ul .box-li .box-word {
    font-size: 24rpx;
    color: #0f0f0f;
}

.box-stylist .box-ul .box-li .box-word .w-1 {
    color: #0f0f0f;
    height: 46rpx;
    line-height: 46rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.box-stylist .box-ul .box-li .box-word .w-1 .w-n {
    font-size: 30rpx;
    height: 46rpx;
    margin-right: 8rpx;
}

.box-stylist .box-ul .box-li .box-word .w-1 .w-h {
    font-size: 30rpx;
    height: 46rpx;
    color: #666;
}

.box-stylist .box-ul .box-li .box-word .w-2 {
    color: #666;
    height: 46rpx;
    line-height: 46rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.box-stylist .box-ul .box-li .box-word .w-3 {
    color: #666;
    height: 46rpx;
    line-height: 46rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.box-stylist .box-ul .box-li .box-tel {
    position: absolute;
    top: 50rpx;
    right: 30rpx;
    font-size: 26rpx;
    color: #0f0f0f;
    text-align: center;
}

.box-stylist .box-ul .box-li .box-tel .icon {
    font-size: 44rpx;
    color: #0f0f0f;
    font-weight: bold;
}