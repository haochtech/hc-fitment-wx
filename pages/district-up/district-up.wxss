page {
    background-color: #eee;
}

.district-up {
    color: #666;
    font-size: 28rpx;
}

.district-up .main {
    background-color: #fff;
    padding-bottom: 30rpx;
    overflow: hidden;
}

.district-up .main .top {
    border-bottom: 1px solid #eee;
    font-size: 32rpx;
}

.district-up .submit {
    height: 90rpx;
    line-height: 90rpx;
    font-size: 32rpx;
    width: 100%;
    border-radius: 0;
    position: fixed;
    left: 0;
    width: 100%;
    box-sizing: border-box;
}

.jieduan-select {
    position: relative;
    width: 73%;
    height: 90rpx;
    line-height: 90rpx;
    text-align: right;
    padding-right: 50rpx;
    box-sizing: border-box;
}

.jieduan-select .iconfont {
    font-size: 30rpx;
    position: absolute;
    right: 0;
    top: 0;
}

.district-up textarea.msg {
    width: 100vw;
    box-sizing: border-box;
    padding: 20rpx;
    height: 300rpx;
    background-color: #fff;
}

.container {
    width: 100vw;
    box-sizing: border-box;
    padding: 0 20rpx;
}

.flex-c {
    display: flex;
    align-items: center;
}

.flex-m {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.imgs {
    display: flex;
    padding: 20rpx;
    background: #eee;
    flex-wrap: wrap;
}

.imgs .img {
    width: 32.3%;
    margin-right: 1%;
    margin-bottom: .5%;
}

.imgs .img-item {
    width: 100%;
    margin-bottom: 18rpx;
    height: 30vw;
}

.imgs .iconfont {
    display: block;
    text-align: center;
    font-size: 40rpx;
    color: #999;
}

.bottom {
    height: 120rpx;
}

.bottom.ipx {
    height: 168rpx;
}

.imgs image {
    width: 100%;
    height: 100%;
}

.hide {
    height: 0;
    width: 0;
    line-height: 0;
    border: none;
    background: none;
    min-height: 0;
    padding: 0;
    margin: 0;
    font-size: 0;
}

.date-time {
    padding: 0 20rpx;
    margin-top: 30rpx;
    font-size: 32rpx;
}

.date-time .d-t {
    font-size: 30rpx;
    color: #999;
}

.date-time .date {
    border-right: 1px solid #eee;
    margin-right: 20rpx;
    padding: 30rpx 20rpx 30rpx 0;
}

.date-time .time {
    margin-right: 10rpx;
}

.up-file {
    margin-top: 20rpx;
}

.up-file .items {
    display: flex;
    margin-bottom: 20rpx;
}

.up-file .items .item {
    width: 150rpx;
    height: 150rpx;
    text-align: center;
    background-color: #eee;
    margin-right: 20rpx;
}

.up-file .items .item .icon {
    width: 70rpx;
    height: 70rpx;
    border-radius: 50%;
    background: #cecdcd;
    line-height: 70rpx;
    text-align: center;
    margin: 20rpx auto 10rpx;
}

.up-file .item .tip {
    font-size: 24rpx;
}

.up-file .items .item .icon .iconfont {
    color: #fff;
    font-size: 50rpx;
}