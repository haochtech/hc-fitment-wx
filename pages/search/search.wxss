@font-face {
    font-family: "iconfont";
    src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

page {
    background: #eee;
}

.quanjing {
    min-height: 55vh;
    padding-top: 250rpx;
}

.search-box {
    padding: 30rpx 30rpx 16rpx;
    font-size: 32rpx;
    background: #fff;
}

.search-box .flex {
    width: 600rpx;
    border: 1px solid #ddd;
    border-radius: 3px;
    box-sizing: border-box;
    padding: 10rpx 16rpx;
}

.search-box .quxiao {
    color: #2fbd80;
}

.search-box .iconfont {
    font-size: 32rpx;
}

.search-box input {
    padding: 0 16rpx;
}

.flex-main {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.flex {
    display: flex;
    align-items: center;
}

.icon-cx:before {
    content: "\e519";
}

.box-pic-header {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    z-index: 80;
    background: #fff;
}

.box-tab-pic {
    background: #fff;
    overflow: hidden;
    display: flex;
    width: 100%;
    border-bottom: 2rpx solid #f5f5f5;
}

.box-tab-pic .box-li {
    width: 100%;
    height: 100%;
    text-align: center;
    font-size: 0;
}

.box-tab-pic .box-li .box-item {
    font-size: 32rpx;
    color: #333;
    line-height: 106rpx;
    text-align: center;
    display: inline-block;
}

.box-tab-pic .box-li.active .box-item {
    color: #2fbd80;
}

.box-pic-list-multi,.box-pic-list-single {
    overflow: hidden;
    padding: 250rpx 0 20rpx;
}

.box-shadow {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background: rgba(0,0,0,.5);
    z-index: 70;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}