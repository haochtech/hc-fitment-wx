@font-face {
    font-family: "iconfont";
    src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

.iconfont {
    font-family: "iconfont"!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-cx:before {
    content: "\e519";
}

.box-pic-header {
    position: fixed;
    top: 0;
    right: 0;
    left: 0;
    z-index: 80;
}

.quanjing {
    padding-top: 120rpx;
}

.box-tab-pic {
    background: #fff;
    overflow: hidden;
    display: flex;
    width: 100%;
}

.box-tab-pic .box-li {
    height: 100%;
    text-align: center;
    font-size: 0;
}

.box-tab-pic .box-li .box-item {
    width: 100%;
    font-size: 32rpx;
    color: #333;
    line-height: 106rpx;
    text-align: center;
    display: inline-block;
}

.box-tab-pic .box-li.active .box-item {
    color: #2fbd80;
}

.box-pic-multi {
    position: relative;
}

.box-pic-multi .box-options {
    background: #fff;
    display: flex;
    width: 100%;
    border-top: 2rpx solid #f5f5f5;
    border-bottom: 2rpx solid #f5f5f5;
}

.box-options {
    position: relative;
}

.box-options .box-options-body {
    position: absolute;
    width: 100vw;
    background: #fff;
    top: 94rpx;
    left: 0;
    height: 0;
    z-index: 999;
    max-height: 70vh;
    overflow: auto;
}

.box-tab-pic .box-li .box-item i.iconfont {
    font-size: 32rpx;
}

.box-options .box-options-body .box-ul {
    padding-top: 30rpx;
}

.box-options .box-options-body.show {
    transition: all .3s;
}

.box-pic-multi .box-options .box-li {
    width: 100%;
    height: 100%;
    text-align: center;
    font-size: 0;
}

.box-pic-multi .box-options .box-li .box-item {
    font-size: 24rpx;
    color: #333;
    line-height: 90rpx;
    text-align: center;
    display: inline-block;
}

.box-pic-multi .box-options .box-li .box-item .icon {
    font-size: 20rpx;
    margin-left: 4rpx;
}

.box-pic-multi .box-options-body .box-ul {
    width: 100%;
    max-height: 70vh;
    overflow: auto;
}

.box-pic-multi .box-options-body .box-li {
    width: 170rpx;
    height: 60rpx;
    line-height: 60rpx;
    margin: 20rpx 38rpx;
    color: #404040;
    border: 2rpx solid #eee;
    float: left;
    border-radius: 6rpx;
    font-size: 24rpx;
}

.box-pic-multi .box-options-body .box-li.active {
    border-color: #2fbd80;
    color: #2fbd80;
}

.box-pic-single {
    position: relative;
}

.box-pic-single .box-options {
    background: #fff;
    display: flex;
    width: 100%;
    border-top: 2rpx solid #f5f5f5;
    border-bottom: 2rpx solid #f5f5f5;
}

.box-pic-single .box-options .box-li {
    width: 100%;
    height: 100%;
