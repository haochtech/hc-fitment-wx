}

.box-pic-single .box-options .box-li .box-item {
    font-size: 24rpx;
    color: #333;
    line-height: 90rpx;
    text-align: center;
    display: inline-block;
}

.box-pic-single .box-options .box-li .box-item .icon {
    font-size: 20rpx;
    margin-left: 4rpx;
}

.box-pic-single .box-options-body .box-ul {
    width: 100%;
    max-height: 70vh;
    overflow: auto;
}

.box-pic-single .box-options-body .box-li {
    width: 170rpx;
    height: 60rpx;
